{
  "name": "fitness-tracker",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "yarn workspace @fitness-tracker/client dev",
    "dev:server": "yarn workspace @fitness-tracker/server dev",
    "dev:all": "concurrently \"yarn dev\" \"yarn dev:server\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}